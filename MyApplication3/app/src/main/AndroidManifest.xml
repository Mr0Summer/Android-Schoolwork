<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrsummer.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".secondActivity"></activity>
        <activity android:name=".thirdActivity"></activity>
        <activity android:name=".forthActivity"></activity>
        <activity android:name=".fifthActivity"></activity>
        <activity android:name=".sixthActivity"></activity>
        <activity android:name=".horizontal"></activity>
        <activity android:name=".left"></activity>
        >
    </application>

</manifest>